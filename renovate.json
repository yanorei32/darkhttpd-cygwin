{
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "darkhttpd.cygport"
      ],
      "extractVersion": "v(?<version>.*?)"
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "darkhttpd.cygport"
      ],
      "matchStrings": [
        "VERSION=(?<currentValue>.*?)"
      ],
      "depNameTemplate": "emikulic/darkhttpd",
      "versioningTemplate": "docker",
      "datasourceTemplate": "github-releases"
    }
  ]
}
